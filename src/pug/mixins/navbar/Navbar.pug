mixin Navbar 
  .sticky-top.navbar-elixir
    .container 
      nav.navbar.navbar-expand-lg 
        a.navbar-brand(href='/index.html')
          img(src=`${CWD}assets/img/logo-dark.png` alt="")
        button.navbar-toggler.p-1(type='button' data-bs-toggle='collapse' data-bs-target='#navbarNavDropdown' aria-controls='navbarNavDropdown' aria-expanded='false' aria-label='Toggle navigation')
          .hamburger.hamburger--emphatic
            .hamburger-box
              .hamburger-inner
        #navbarNavDropdown.collapse.navbar-collapse
          ul.navbar-nav.fs-0.fw-bold.py-3.py-lg-0.mt-1.mb-2.my-lg-0
            li.nav-item.dropdown
              a.nav-link.dropdown-toggle.dropdown-indicator(href='JavaScript:void(0)' role="button" data-bs-toggle="dropdown" aria-expanded="false") Home
              ul.dropdown-menu.mt-n1(aria-labelledby='navbarDropdown')
                li
                  a.dropdown-item(href="/index.html") Slider Header
                li
                  a.dropdown-item(href="/homes/header-slider-classic.html") Slider Classic
                li
                  a.dropdown-item(href="/homes/header-static.html") Static Header
                li
                  a.dropdown-item(href="/homes/header-static-classic.html") Static Classic
                li
                  a.dropdown-item(href="/homes/header-youtube-video.html") Youtube Background 
                li
                  a.dropdown-item(href="/homes/header-youtube-video-classic.html") Youtube Classic
                li
                  a.dropdown-item(href="/homes/header-selfhosted-video.html") Self-hosted Video
                li
                  a.dropdown-item(href="/homes/header-selfhosted-video-classic.html") Self-hosted Classic

            li.nav-item.dropdown
              a.nav-link.dropdown-toggle.dropdown-indicator(href='JavaScript:void(0)' role="button" data-bs-toggle="dropdown" aria-expanded="false") Pages
              ul.dropdown-menu.mt-n1(aria-labelledby='navbarDropdown')
                li
                  a.dropdown-item(href="/pages/service.html") Services
                li
                  a.dropdown-item(href="/pages/about.html") About
                li
                  a.dropdown-item(href="/pages/alumni.html") Alumni
                li
                  a.dropdown-item(href="/pages/blank.html") Blank Page
                li
                  a.dropdown-item(href="/pages/404.html") 404 Page
                li
                  a.dropdown-item(href="/pages/login.html") Login
                li
                  a.dropdown-item(href="/pages/registration.html") Registration

            li.nav-item.dropdown
              a.nav-link.dropdown-toggle.dropdown-indicator(href='JavaScript:void(0)' role="button" data-bs-toggle="dropdown" aria-expanded="false") News
              ul.dropdown-menu.mt-n1(aria-labelledby='navbarDropdown')
                li
                  a.dropdown-item(href="/news/newsroom.html") Newsroom
                li
                  a.dropdown-item(href="/news/news.html") Single News

            li.nav-item.dropdown
              a.nav-link.dropdown-toggle.dropdown-indicator(href='JavaScript:void(0)' role="button" data-bs-toggle="dropdown" aria-expanded="false") Elements
              ul.dropdown-menu.mt-n1(aria-labelledby='navbarDropdown')
                li
                  a.dropdown-item(href="/elements/buttons.html") Buttons
                li
                  a.dropdown-item(href="/elements/colors.html") Colors
                li
                  a.dropdown-item(href="/elements/googlemap.html") Google Map
                li
                  a.dropdown-item(href="/elements/grid.html") Grid
                li
                  a.dropdown-item(href="/elements/icons.html") Icons
                li
                  a.dropdown-item(href="/elements/layouthelpers.html") Layout Helpers
                li
                  a.dropdown-item(href="/elements/modal-video.html") Modal Video
                li
                  a.dropdown-item(href="/elements/swiper.html") Swiper
                li
                  a.dropdown-item(href="/elements/typography.html") Typography

            li.nav-item
              a.nav-link(href='contact.html').d-block.mr-md-9 Contact
          ul.navbar-nav.ms-lg-auto.py-3.py-lg-0.my-1.my-lg-0 
            li.nav-item
              a(href='https://themewagon.com/themes/elixir/' target="_blank").btn.btn-outline-primary.rounded-pill.btn-sm.border-2.fw-bold.w-100.w-lg-auto Purchase